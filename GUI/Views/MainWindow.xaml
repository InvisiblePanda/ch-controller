<Window x:Class="GUI.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI.Views"
        mc:Ignorable="d"
        Title="MainWindow" Height="202" Width="200">
    <StackPanel>
        <Button Command="{Binding AutoFireCmd}" Height="50">Turn on AutoFire</Button>
        <Button Command="{Binding CheckForFishCmd}" Height="50">Test</Button>
        <Button Command="{Binding ShowMaskWindowCmd}" Height="50">Show Mask</Button>
    </StackPanel>
</Window>